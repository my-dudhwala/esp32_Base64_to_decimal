void Base64(byte FourBytes[]) {
  
  // Step 1: Remove 2 prefix bits, create 4 sets of 6 bits, and concatenate into a 24-bit variable
  unsigned long concatenatedValue = 0;
  for (int i = 0; i < 4; i++) {
    // Remove 2 prefix bits
    byte sixBits = FourBytes[i] & 0b00111111;
    concatenatedValue <<= 6; // Shift left by 6 bits
    concatenatedValue |= sixBits;
  }

  // Step 2: Separate the 24-bit variable into 3 bytes
  byte byte1 = (concatenatedValue >> 16) & 0xFF;
  byte byte2 = (concatenatedValue >> 8) & 0xFF;
  byte byte3 = concatenatedValue & 0xFF;

